{
  "name": "react-front-fera",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "build": "npm run env:prod -- webpack",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "dev": "npm run env:dev -- webpack-dev-server",
    "prod": "npm run env:prod -- webpack-dev-server",
    "eject": "react-scripts eject",
    "deploy": "serve -s build -l 8080",
    "fix:other": "yarn prettier --write",
    "fix:js": "yarn test:js --fix",
    "fix": "yarn fix:js && yarn fix:other",
    "prettier": "prettier \"**/*.{json,md,scss,yaml,yml}\"",
    "test:other": "yarn prettier --list-different",
    "test:js": "eslint --ignore-path .gitignore --ignore-path .prettierignore \"**/*.{js,jsx}\"",
    "test": "yarn test:other && yarn test:js"
  },
  "dependencies": {
    "@date-io/moment": "^1.3.11",
    "@material-ui/core": "^4.4.1",
    "@material-ui/icons": "^4.4.1",
    "@material-ui/lab": "^4.0.0-alpha.26",
    "@material-ui/styles": "^4.4.1",
    "axios": "^0.19.0",
    "babel-preset-react": "^6.24.1",
    "clsx": "^1.0.4",
    "downshift": "^3.3.4",
    "eslint-plugin-react": "^7.14.3",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "lodash-move": "^1.1.1",
    "material-ui-dropzone": "^2.4.5",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "3.1.1",
    "react-select": "^3.0.8",
    "react-select-material-ui": "^6.0.1",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "serve": "^11.1.0",
    "styled-components": "^4.4.1",
    "styled-theme": "^0.3.3",
    "styled-tools": "^1.7.1",
    "uuid": "^3.3.3",
    "warning": "^4.0.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
